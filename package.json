{
  "name": "skicyclerun",
  "description": "An Adventum site",
  "homepage": "https://skicyclerun.com",
  "type": "module",
  "version": "0.5.8",
  "scripts": {
    "dev": "astro dev",
    "devs": "astro dev --https --cert skicyclerun_cert.pem --key skicyclerun_key.pem",
    "start": "astro dev",
    "build": "astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "format": "prettier --plugin-search-dir=. --write .",
    "cz": "cz",
    "lint": "eslint ."
  },
  "dependencies": {
    "@astrojs/check": "latest",
    "@astrojs/mdx": "latest",
    "@astrojs/node": "latest",
    "@astrojs/rss": "latest",
    "@aws-amplify/adapter-nextjs": "latest",
    "@aws-amplify/auth": "latest",
    "@aws-amplify/ui-react": "latest",
    "@resvg/resvg-js": "latest",
    "astro-embed": "latest",
    "astro-icon": "latest",
    "aws-amplify": "latest",
    "fuse.js": "latest",
    "marked": "^16.2.1",
    "overlayscrollbars": "latest",
    "remark-collapse": "latest",
    "remark-toc": "latest",
    "satori": "latest"
  },
  "devDependencies": {
    "@astrojs/react": "^4.3.1",
    "@astrojs/sitemap": "latest",
    "@astrojs/tailwind": "latest",
    "@aws-amplify/backend": "latest",
    "@aws-amplify/backend-cli": "latest",
    "@divriots/jampack": "latest",
    "@rollup/plugin-replace": "latest",
    "@tailwindcss/typography": "latest",
    "@types/github-slugger": "latest",
    "@types/marked": "^5.0.2",
    "@types/react": "latest",
    "@typescript-eslint/parser": "latest",
    "astro": "latest",
    "astro-eslint-parser": "latest",
    "aws-cdk-lib": "latest",
    "commitizen": "latest",
    "constructs": "latest",
    "cz-conventional-changelog": "latest",
    "esbuild": "latest",
    "eslint": "latest",
    "eslint-plugin-astro": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "prettier-plugin-astro": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "latest",
    "tsx": "latest",
    "typescript": "latest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json}": [
      "prettier --plugin-search-dir=. --write"
    ]
  }
}
