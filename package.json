{
  "name": "skicyclerun",
  "description": "An Adventum site",
  "homepage": "https://skicyclerun.com",
  "type": "module",
  "version": "0.5.5",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "format": "prettier --plugin-search-dir=. --write .",
    "cz": "cz",
    "lint": "eslint ."
  },
  "dependencies": {
    "@astrojs/check": "latest",
    "@astrojs/mdx": "latest",
    "@astrojs/rss": "latest",
    "@resvg/resvg-js": "latest",
    "astro": "latest",
    "fuse.js": "latest",
    "github-slugger": "latest",
    "remark-collapse": "latest",
    "remark-toc": "latest",
    "satori": "latest",
    "tailwindcss": "latest",
    "tw-elements": "^2.0.0",
    "typescript": "latest"
  },
  "devDependencies": {
    "@astrojs/react": "latest",
    "@astrojs/sitemap": "latest",
    "@astrojs/tailwind": "latest",
    "@divriots/jampack": "latest",
    "@tailwindcss/typography": "latest",
    "@types/github-slugger": "latest",
    "@types/react": "latest",
    "@typescript-eslint/parser": "latest",
    "astro-eslint-parser": "latest",
    "commitizen": "latest",
    "cz-conventional-changelog": "latest",
    "eslint": "latest",
    "eslint-plugin-astro": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "prettier-plugin-astro": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json}": [
      "prettier --plugin-search-dir=. --write"
    ]
  }
}
