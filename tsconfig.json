{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": "src",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["astro/client"],
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@config/*": ["config/*"],
      "@constants/*": ["constants/*"],
      "@content/*": ["content/*"],
      "@images/*": ["assets/images/*"],
      "@layouts/*": ["layouts/*"],
      "@locales/*": ["locales/*"],
      "@pages/*": ["pages/*"],
      "@scripts/*": ["scripts/*"],
      "@skicyclerun.config": ["skicyclerun.config.ts"],
      "@styles/*": ["styles/*"],
      "@types/*": ["types/*"],
      "@utils/*": ["utils/*"],
      "@svg_imgs/*": ["assets/svg_imgs/*"]
    }
  }
}
